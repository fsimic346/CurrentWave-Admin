{
  "name": "admin-panel",
  "version": "0.0.0",
  "main": "main.js",
  "build": {
    "win": {
      "icon": "dist/currentwave/browser/favicon.ico"
    }},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build  --base-href /CurrentWave-Admin/",
